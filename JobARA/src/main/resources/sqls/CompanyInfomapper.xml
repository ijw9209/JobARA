<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="Company">
	
	<insert id="companyinsert" parameterType="CompanyInfoDto">
	INSERT INTO COMPANY_INFO VALUES(#{member_no_seq},COMPANYNOSEQ.NEXTVAL,#{company_title},#{company_name},#{company_cate},#{company_capital},#{company_take},#{company_regdate},#{company_all_people},#{company_salary},#{company_url})
	</insert>

</mapper>
